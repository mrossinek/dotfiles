###########################
# xbindkeys configuration #
###########################

"pactl set-sink-mute @DEFAULT_SINK@ toggle"
        XF86AudioMute

"pactl set-sink-volume @DEFAULT_SINK@ +2%"
        XF86AudioRaiseVolume

"pactl set-sink-volume @DEFAULT_SINK@ -2%"
        XF86AudioLowerVolume

"xbacklight -inc 5"
        XF86MonBrightnessUp

"xbacklight -dec 5"
        XF86MonBrightnessDown

"maim -uks ~/Pictures/Screenshots/screenshot_$(date +%s).png"
        Mod4 + Print

"physlock -m"
        Mod4 + 0

"~/keyboard_toggle.sh"
        XF86Launch2

"~/toggle_screen.sh"
        XF86Display

"~/xrandr_duplicate.sh"
        Shift + XF86Display

##################################
# End of xbindkeys configuration #
##################################
